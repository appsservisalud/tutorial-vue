{
  "name": "tutorial-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "parcel build --no-scope-hoist ./src/index.html --dist-dir ./appsscript && npm run clean",
    "clean": "rimraf ./appsscript/*.css  ./appsscript/*.map",
    "gcreate": "clasp create --title \"tutorial-vue\" --rootDir ./appsscript",
    "gdeploys": "clasp deployments",
    "ghelp": "clasp --help",
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gopen": "clasp open --webapp",
    "gpull": "clasp pull",
    "gpush": "clasp deploy --deploymentId   AKfycbyuZ2ZiJeKK3yIV6PcmD73a52txM0gi5MzYe2QCjRxo --versionNumber 0 ",
    "gstart": "clasp push --watch",
    "sass": "sass --watch src/assets/scss/:src/assets/css/",
    "start": "nodemon --watch ./src -e html,js,css,scss,vue --exec \"npm run build\" ",
    "vbuild": "vue-cli-service build"
  },
  "dependencies": {
    "@types/google-apps-script": "^1.0.55",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "core-js": "^3.8.3",
    "datatables.net-dt": "^1.12.1",
    "datatables.net-vue3": "^1.0.0",
    "jquery": "^3.6.1",
    "materialize-css": "^1.0.0-rc.2",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "vue": "^3.2.13",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.7.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "parcel": "^2.7.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2"
  }
}